# Backend listen address
MQLM_ADDR=127.0.0.1:8080

# SQLite path (relative to backend/ is fine)
MQLM_DB_PATH=./data/mql_manager.db

# Admin token for Authorization: Bearer <token>
MQLM_ADMIN_TOKEN=change-me

# CORS (comma-separated origins)
MQLM_CORS_ORIGINS=http://127.0.0.1:3000,http://localhost:3000
